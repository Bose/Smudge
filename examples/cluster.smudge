// Example of a smudgule with more than one state machine in it.

SMA
{
  * A [ dummy --> B, _ --> B ],
    B [ _ --> A ]
}

SMB
{
  * A [ dummy --> B, _ --> B ],
    B [ _ --> C ],
    C [ _ --> D ],
    D [ _ --> A ]
}
